<!DOCTYPE html>
<html>
	<head>
	  <title>WODapp</title>
	  <%= stylesheet_link_tag 'application', media: 'all' %>
	  <%= javascript_include_tag 'application' %>
	  <%= csrf_meta_tags %>
	  <%= analytics_init if Rails.env.production? %>
	</head>
	<body>
		<div class="container">
			<header>
		
			</header>  
			<main role="main">
				<div class="decorate">
					<%= render 'layouts/navigation' %>
					<%= render 'layouts/messages' %>
					<%= yield %>
				</div>
			</main>
		</div>
	</body>
</html>
